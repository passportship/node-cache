{
  "name": "@passportship/cache-list",
  "description": "Cache list",
  "version": "2.0.0-beta.1",
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/passportship/node-cache.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build-ts",
    "build-ts": "tsc",
    "clean": "rm -rf lib",
    "dev": "npm run clean && tsc -w",
    "feature-hotfix": "bin/feature-to-hotfix.sh",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src/** --ext ts --quiet",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "release": "export GIT_MERGE_AUTOEDIT=no && npm --no-git-tag-version version patch && RELEASE_VERSION=$(node -p -e \"require('./package.json').version\") && git checkout package.json package-lock.json && git flow release start v$RELEASE_VERSION && npm version patch --message \"bump server version to %s\" && git flow release finish -n v$RELEASE_VERSION && unset GIT_MERGE_AUTOEDIT",
    "start": "npm run dev",
    "test": "npm run build && mocha test"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@passportship/eslint-config-passport-rules": "^3.2.0",
    "@passportship/npm-tools": "^2.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "mocha": "^11.7.5",
    "proxyquire": "^2.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "node",
    "cache",
    "memory cache",
    "redis cache"
  ]
}
